<%= form_for :park_system, url: '/admin/park_system' do |f|%>
    <div class="form-group">
    <%= f.label :"Enter the name of your parking system" %><br>
    <%= f.text_field :name%>
    </div>

    <div class="form-group">
    <%= f.label :"Enter the maximum number of slot available in your parking system" %><br>
    <%= f.text_field :slot_count%>
    </div>
    
    <div class = "action">
    <%= f.submit "create"%>
    </div>
    <% end %>